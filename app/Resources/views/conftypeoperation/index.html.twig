{% extends 'conftypeoperation/template.html.twig' %}
{% block breadcrumb %}
    <div class="container">
        <ol class="breadcrumb">
            <li> {{'menu.dashboard.param'|trans(arguments = {}, domain = 'menu')}} </li>
            <li class="active"> {{'typeoperation.name'|trans(arguments = {}, domain = 'typeoperation')}}  </li>
          </ol>
    </div>
{% endblock %}
{% block content %}
<div class="panel panel-default">
    <div class="panel-heading main-color-bg">
        <h3 class="panel-title">{{'typeoperation.indexview.title'|trans(arguments = {}, domain = 'typeoperation')}}</h3>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-3">
                <a type="button" href="{{ path('typeoperation_new') }}" class="btn main-new-action-bg"> <span class="glyphicon glyphicon-plus-sign"></span> <b> {{'typeoperation.indexview.addbtnlabel'|trans(arguments = {}, domain = 'typeoperation')}}</b></a>
            </div>
        </div>
        <br>
        <div class="row">
            <div class="col-md-12">
                <input class="form-control" type="text" placeholder="{{'typeoperation.indexview.table.searchformlabel'|trans(arguments = {}, domain = 'typeoperation')}}">
            </div>
        </div>
        <br>
      <!--<h1>Liste des compte comptables plans </h1> -->
       <div id="div_tableau_liste_typeoperation">
            {% include 'conftypeoperation/include/tableau.html.twig' with  {'confTypeOperations': confTypeOperations} %}
        </div>
   <!--  <ul>
       <li>
           <a href="{#{ path('conftypeoperation_new') }#}">Create a new conftypeoperation</a>
       </li>
   </ul> -->
    </div>
    </div>

{% endblock %}
