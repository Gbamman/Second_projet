{% extends 'confagence/template.html.twig' %}

{% block breadcrumb %}
    <div class="container">
        <ol class="breadcrumb">
              <li> {{'menu.dashboard.param'|trans(arguments = {}, domain = 'menu')}} </li>
            <li class="active"> {{'agence.name'|trans(arguments = {}, domain = 'agence')}}  </li>
            <li class="active"> {{'agence.showview.title'|trans(arguments = {}, domain = 'agence')}}</li>
        </ol>
    </div>
{% endblock %}

{% block content %}
<div class="panel panel-default">
    <div class="panel-heading main-color-bg">
        <h3 class="panel-title">{{'agence.showview.title'|trans(arguments = {}, domain = 'agence')}}</h3>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-3">
                <a type="button" href="{{ path('agence_index') }}" class="btn main-new-action-bg"> <span class="glyphicon glyphicon-arrow-left"></span> <b>  {{'crud.backbtnlabel'|trans(arguments = {}, domain = 'crud')}} </b></a>
            </div>
            <div class="col-md-3">
                <a type="button" href="{{ path('agence_edit', { 'id': confAgence.id }) }}" class="btn main-new-action-bg"> <span class="glyphicon glyphicon-pencil"></span> <b> {{'crud.editbtnlabel'|trans(arguments = {}, domain = 'crud')}} </b></a>
            </div>
        </div>

        <br>
    <table class="table table-bordered">
        <tbody>
           
            <tr>
                <th>{{'agence.indexview.table.code'|trans(arguments = {}, domain = 'agence') }}</th>
                <td>{{ confAgence.code }}</td>
            </tr>
            <tr>
                <th>{{'agence.indexview.table.libelle'|trans(arguments = {}, domain = 'agence') }}</th>
                <td>{{ confAgence.libelle }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}